{
    "name_app": "Web Ordering App",
    "styles": [],
    "scripts": [],
    "img_default": "none.png",
    "routing": {
        "main": {
            "css": [ "main", "header", "footer", "perfect-scrollbar"],
            "model": ["main","header", "footer"],
            "templates": ["main", "header", "footer"],
            "views": ["main", "header", "footer", "logo"],
            "core": ["geopoint", "jquery_numbermask"],
            "js": ["perfect-scrollbar-0.4.5.with-mousewheel.min"]
        },
        "index": {
            "css": ["store_info", "categories"],
            "views": ["store_info"],
            "templates": ["store_info", "categories"],
            "core": ["categories", "categories_view"]
        },
        "products": {
            "css": ["products"],
            "views": ["products"],
            "templates": ["products"],
            "core": ["categories", "products"]
        },
        "modifiers": {
            "css": ["modifiers", "products", "quantity", "instructions", "myorder"],
            "views": ["products", "quantity", "instructions", "myorder"],
            "templates": ["modifiers", "products", "quantity", "instructions", "myorder"],
            "core": ["categories", "products", "modifiers", "modifiers_view", "instructions_view"]
        },
        "myorder": {
            "css": ["myorder", "total"],
            "views": ["myorder"],
            "templates": ["myorder", "total"],
            "core": ["myorder_view", "total_view"]
        },
        "checkout": {
            "css": ["checkout"],
            "views": ["checkout"],
            "templates": ["checkout"],
            "core": ["checkout", "products", "delivery_addresses", "customers"]
        },
        "card": {
            "css": ["card"],
            "views": ["card"],
            "templates": ["card"],
            "core": ["card", "jquery_numbermask", "card_view"]
        },
        "confirm": {
            "css": ["total", "checkout", "card", "tips"],
            "views": ["checkout", "card"],
            "templates": ["total", "checkout", "card", "tips"],
            "core": ["tip", "checkout", "card", "jquery_numbermask", "total_view", "tips_view"]
        },
        "done": {
            "css": ["done"],
            "views": ["done"],
            "templates": ["done"],
            "core": ["products", "modifiers", "tip", "checkout", "card"]
        },
        "store_info": {
            "css": ["store_info"],
            "views": ["store_info"],
            "templates": ["store_info"],
            "core": ["jquery_gallery", "about"],
            "cssCore": ["libs/jquery/jquery.gallery/jquery.gallery"]
        },
        "pay": {
            "core": ["checkout", "products", "modifiers", "card", "tip", "customers"]
        },
        "maintenance": {
            "css": ["maintenance"],
            "views": ["maintenance"],
            "templates": ["maintenance"]
        }
    }
}